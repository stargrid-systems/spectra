{
  "name": "Spectra",
  "dockerComposeFile": "compose.yaml",
  "service": "devcontainer",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": ["authentik-server:9000", "postgres:5432"],
  "portsAttributes": {
    "9000": {
      "label": "Authentik UI"
    }
  },
  "remoteEnv": {
    "NUXT_DATABASE_URL": "postgresql://dev:dev@postgres:5432/spectra",
    "NUXT_AUTHENTIK_BASE_URL": "http://authentik-server:9000/",
    "NUXT_AUTHENTIK_ACCESS_TOKEN": "398829673032ee4b"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {
      "moby": false
    },
    "ghcr.io/stuartleeks/dev-container-features/shell-history:0": {}
  },
  "customizations": {
    "vscode": {
      "extensions": ["Vue.volar", "Prettier.prettier-vscode"],
      "settings": {
        "editor.defaultFormatter": "prettier.prettier-vscode"
      }
    }
  }
}
